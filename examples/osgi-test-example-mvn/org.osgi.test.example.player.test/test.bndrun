-tester: biz.aQute.tester.junit-platform

-runvm: ${def;argLine}

-runfw: org.eclipse.osgi
-resolve.effective: active
-runproperties: \
	logback.configurationFile=${fileuri;${.}/logback.xml},\
	org.apache.felix.http.host=localhost,\
	org.osgi.service.http.port=*,\
	org.osgi.framework.bootdelegation=sun.reflect,\
	osgi.console=

-runsystempackages: \
	org.slf4j;version=1.7.25,\
	org.slf4j.helpers;version=1.7.25,\
	org.slf4j.spi;version=1.7.25
-runpath: \
	ch.qos.logback.classic,\
	ch.qos.logback.core,\
	org.apache.felix.logback,\
	slf4j.api
-runrequires: \
	bnd.identity;id='${project.artifactId}-tests',\
	bnd.identity;id='junit-jupiter-engine',\
	bnd.identity;id='junit-platform-launcher'
# This will help us keep -runbundles sorted
-runstartlevel: \
    order=sortbynameversion,\
    begin=-1
# The following is calculated by the bnd-resolver-maven-plugin
-runbundles: \
	assertj-core;version='[3.19.0,3.19.1)',\
	junit-jupiter-api;version='[5.7.1,5.7.2)',\
	junit-jupiter-engine;version='[5.7.1,5.7.2)',\
	junit-platform-commons;version='[1.7.1,1.7.2)',\
	junit-platform-engine;version='[1.7.1,1.7.2)',\
	junit-platform-launcher;version='[1.7.1,1.7.2)',\
	net.bytebuddy.byte-buddy;version='[1.10.19,1.10.20)',\
	net.bytebuddy.byte-buddy-agent;version='[1.10.19,1.10.20)',\
	org.apache.felix.scr;version='[2.1.24,2.1.25)',\
	org.mockito.mockito-core;version='[3.7.7,3.7.8)',\
	org.objenesis;version='[3.1.0,3.1.1)',\
	org.opentest4j;version='[1.2.0,1.2.1)',\
	org.osgi.test.assertj.framework;version='[0.10.0,0.10.1)',\
	org.osgi.test.common;version='[0.10.0,0.10.1)',\
	org.osgi.test.example.api;version='[0.1.0,0.1.1)',\
	org.osgi.test.example.player.impl;version='[0.1.0,0.1.1)',\
	org.osgi.test.example.player.test-tests;version='[0.1.0,0.1.1)',\
	org.osgi.test.junit5;version='[0.10.0,0.10.1)',\
	org.osgi.util.function;version='[1.1.0,1.1.1)',\
	org.osgi.util.promise;version='[1.1.1,1.1.2)'
